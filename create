echo "#include<bits/stdc++.h>" >> $1
echo "using namespace std;" >> $1
echo "" >> $1
echo "int main(void)" >> $1
echo "{" >> $1
echo "        ios_base::sync_with_stdio(false);" >> $1
echo "        cin.tie(0);" >> $1
echo "        int t;" >> $1
echo "        cin >> t;" >> $1
echo "        while(t--)" >> $1
echo "        {"  >> $1
echo "" >> $1
echo "        }"  >> $1
echo "        return 0;" >> $1
echo "}" >> $1
vim $1
